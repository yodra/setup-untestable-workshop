{
  "name": "setup-untestable-workshop",
  "version": "1.0.0",
  "description": "setup untestable workshop",
  "keywords": [
    "kata",
    "tests",
    "workshop",
    "untestable"
  ],
  "main": "main.ts",
  "engineStrict": true,
  "engines": {
    "node": ">= 18.17.1",
    "npm": ">= 9.6.7"
  },
  "contributors": [
    "yodra"
  ],
  "license": "ISC",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "dependencies": {
    "moment": "2.29.4"
  },
  "devDependencies": {
    "@cowcoders/eslint-config": "1.2.9",
    "@types/jest": "26.0.23",
    "@types/node": "15.6.0",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "eslint": "7.27.0",
    "eslint-plugin-jest": "24.3.6",
    "jest": "26.6.3",
    "shx": "0.3.3",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  },
  "jest": {
    "clearMocks": true,
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testMatch": [
      "**/*.test.(j|t)s"
    ],
    "verbose": true
  }
}
